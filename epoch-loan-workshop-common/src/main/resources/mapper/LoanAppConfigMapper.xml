<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.epoch.loan.workshop.common.dao.mysql.LoanAppConfigDao">

    <select id="findByAppName" resultType="com.epoch.loan.workshop.common.entity.mysql.LoanAppConfigEntity">
        SELECT t1.* FROM platform_app_config t1 LEFT JOIN platform_app t2 on t1.app_id = t2.id WHERE t2.`name` = #{appFlag}
    </select>
</mapper>