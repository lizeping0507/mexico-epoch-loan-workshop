<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.epoch.loan.workshop.common.dao.mysql.LoanRemittanceBankDao">

    <select id="findLoanRemittanceBankList"
            resultType="com.epoch.loan.workshop.common.entity.mysql.LoanRemittanceBankEntity">
        SELECT * FROM loan_remittance_bank where user_id = #{userId}
    </select>
    <select id="findByName" resultType="com.epoch.loan.workshop.common.entity.mysql.LoanRemittanceBankEntity">
        SELECT * FROM loan_remittance_bank WHERE name = #{name} LIMIT 1
    </select>
</mapper>