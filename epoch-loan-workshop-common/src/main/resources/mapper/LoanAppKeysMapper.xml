<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.epoch.loan.workshop.common.dao.mysql.LoanAppKeysDao">


    <select id="getLoanAppKeyByAppName"
            resultType="com.epoch.loan.workshop.common.entity.mysql.LoanAppKeysEntity">
