<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.epoch.loan.workshop.common.dao.mysql.LoanOcrProviderConfigDao">

    <select id="findProviderConfig"
            resultType="com.epoch.loan.workshop.common.entity.mysql.LoanOcrProviderConfig">
        SELECT * FROM loan_ocr_provider_config WHERE app_name = #{appName} AND  status = 1
    </select>

    <select id="selectAdvanceConfig"
            resultType="java.lang.String">
        SELECT * FROM loan_ocr_provider_config WHERE app_name = #{appName} AND  status = 1 AND channel_code = 1
    </select>
</mapper>