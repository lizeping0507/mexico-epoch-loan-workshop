<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.epoch.loan.workshop.common.dao.mysql.LoanAppControlDao">

    <select id="findByAppNameAndAppVersion"
            resultType="com.epoch.loan.workshop.common.entity.mysql.LoanAppControlEntity">
        SELECT * FROM `loan_app_control` WHERE app_name = #{appName} AND app_version = #{appVersion}
    </select>
</mapper>