<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.epoch.loan.workshop.common.dao.PlatformMerchantApiUrlDao">

    <select id="findMerchantApiUrl" resultType="com.epoch.loan.workshop.common.entity.PlatformMerchantApiUrlEntity">
        SELECT * FROM platform_merchant_api_url WHERE merchant_id=#{merchantId} LIMIT 0,1
    </select>
</mapper>