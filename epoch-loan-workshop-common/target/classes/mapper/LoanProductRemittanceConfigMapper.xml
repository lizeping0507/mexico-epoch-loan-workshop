<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.epoch.loan.workshop.common.dao.LoanProductRemittanceConfigDao">


    <select id="findByGroupName"
            resultType="com.epoch.loan.workshop.common.entity.LoanProductRemittanceConfigEntity">
        SELECT *
        FROM loan_product_remittance_config
        WHERE group_name = #{groupName}
        LIMIT 0,1
    </select>
</mapper>